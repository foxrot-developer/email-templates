<div style="background-image: url(https://i.postimg.cc/c42QBCMX/Okay.png);
background-position: center;
background-repeat: no-repeat;
background-size: cover;
width: 100%;
height: 150px;
text-align: start;
padding: 30px;">
    <div style="display: flex;
    align-items: center;">
        <div>
            <p style="font-family: 'Poppins';
        font-style: normal;
        font-weight: 600;
        font-size: 14px;
        line-height: 20px;
        color: #FFFFFF;
        margin-right: 20px;">Addison Daniels</p>
            <p style="font-family: 'Poppins';
        font-style: normal;
        font-weight: 400;
        font-size: 12px;
        line-height: 16px;
        letter-spacing: 0.01em;
        color: #FAFAFA;">Cloud Engineer</p>
        </div>
        <div>
            <img src="https://i.postimg.cc/ZYf61pJm/addison.png" alt="Addison" loading="lazy"
                style="margin-top: 15px;" />
        </div>
    </div>
</div>
<div style="background-color: #E5E5E5;
padding: 0 20px;
margin-top: -20px;">
    <div style="background-color: #fff; padding: 20px 0;">
        <div style="text-align: center;">
            <img src="https://i.postimg.cc/9QYK6dnx/techchak-01-2.png" style="width: 120px;
            height: 24px;" />
        </div>
        <div style="padding: 20px 20px;">
            <p style="font-family: 'Poppins';
            font-style: normal;
            font-weight: 700;
            font-size: 16px;
            line-height: 24px;
            color: #17191C;">
                Your Wiki has been successfully Created and published.
            </p>
            <div style="display: flex;">
                <img src="<%= wiki.wiki.image %>" alt="Wiki" loading="lazy" style="margin-right: 20px;" />
                <div>
                    <p style="font-family: 'Poppins';
                    font-style: normal;
                    font-weight: 700;
                    font-size: 16px;
                    line-height: 24px;
                    color: #17191C;">
                        <%= wiki.wiki.title %>
                    </p>
                    <p style="font-family: 'Poppins';
                    font-style: normal;
                    font-weight: 400;
                    font-size: 10px;
                    line-height: 15px;
                    color: #464646;">
                        <%= wiki.wiki.description %>
                    </p>
                </div>
            </div>
            <h2 style="font-family: 'Poppins';
            font-style: normal;
            font-weight: 600;
            font-size: 14px;
            line-height: 20px;
            color: #17191C;
            margin-top: 30px;">Similar Wikis</h2>
            <div style="display: flex;">
                <% wiki.similar.forEach(function(post) { %>
                    <div style="margin: 0 10px;">
                        <img src="<%= post.image %>" style="width: 100%; height: 200px;" />
                        <p style="font-family: 'Poppins';
                        font-style: normal;
                        font-weight: 600;
                        font-size: 14px;
                        line-height: 20px;
                        color: #17191C;">
                            <%= post.title %>
                        </p>
                        <p style="font-family: 'Poppins';
                        font-style: normal;
                        font-weight: 400;
                        font-size: 10px;
                        line-height: 15px;
                        color: #464646;">
                            <%= post.description %>
                        </p>
                    </div>
                    <% }); %>
            </div>
            <button style="background: #42C3FF;
                border-radius: 8px;
                width: 280px;
                height: 48px;
                color: #fff;
                border: none;
                cursor: pointer;
                padding: 12px 32px;
                margin-top: 30px;">View More</button>
        </div>
    </div>
</div>
</div>